@model AppCore.Models.QT_HOSOTIEPNHAN

<div id="divXuLyHoSoID" class="m-15">
    <div class="row panel" style="margin-bottom: 5px;">
        <div class="btn btn-default btn-block tab-doc" style="text-align:left;">
            <a style="color: blue;font-weight: bold;" data-toggle="collapse" data-parent="#divXuLyHoSoID" href="#collapse_XuLyHoSo_TTTiepNhan">
                Thông tin tiếp nhận hồ sơ
            </a>
        </div>
        <div id="collapse_XuLyHoSo_TTTiepNhan" class="panel-collapse collapse in">
            <div id="divXuLyHoSoID_TTTiepNhan">
                <div class="box box-primary">
                    <div class="box-body">
                        <div class="row">
                            <div class="col-xs-2">
                                @Html.LabelFor(model => model.SOBIENNHAN, "Số biên nhận hồ sơ", new { @class = "pull-right control-label" })
                            </div>
                            <div class="col-xs-4">
                                @Html.TextBoxFor(model => model.SOBIENNHAN, "", new { @class = "form-control input-sm" })
                            </div>
                            <div class="col-xs-2">
                                @*@Html.LabelFor(model => model., "Số biên nhận hồ sơ", new { @class = "pull-right control-label" })*@
                            </div>
                            <div class="col-xs-4">
                                @*@Html.TextBoxFor(model => model.SOBIENNHAN, "", new { @class = "form-control input-sm" })*@
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-2">
                                @Html.LabelFor(model => model.NGUOINOPHOSO, "Người nộp hồ sơ", new { @class = "pull-right control-label" })
                            </div>
                            <div class="col-xs-4">
                                @Html.TextBoxFor(model => model.NGUOINOPHOSO, "", new { @class = "form-control input-sm" })
                            </div>
                            <div class="col-xs-2">
                                @Html.LabelFor(model => model.DIACHINGUOINOPHS, "Địa chỉ", new { @class = "pull-right control-label" })
                            </div>
                            <div class="col-xs-4">
                                @Html.TextBoxFor(model => model.DIACHINGUOINOPHS, "", new { @class = "form-control input-sm" })
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-2">
                                @Html.LabelFor(model => model.SODIENTHOAI, "Số điện thoại", new { @class = "pull-right control-label" })
                            </div>
                            <div class="col-xs-4">
                                @Html.TextBoxFor(model => model.SODIENTHOAI, "", new { @class = "form-control input-sm" })
                            </div>
                            <div class="col-xs-2">
                                @Html.LabelFor(model => model.EMAIL, "Email", new { @class = "pull-right control-label" })
                            </div>
                            <div class="col-xs-4">
                                @Html.TextBoxFor(model => model.EMAIL, "", new { @class = "form-control input-sm" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row panel" style="margin-bottom: 5px;">
        <div class="btn btn-default btn-block tab-doc" style="text-align:left;">
            <a style="color: blue;font-weight: bold;" data-toggle="collapse" data-parent="#divXuLyHoSoID" href="#collapse_XuLyHoSo_DSFileDinhKem">
                Danh sách file đính kèm
            </a>
        </div>
        <div id="collapse_XuLyHoSo_DSFileDinhKem" class="panel-collapse collapse">
            <div id="divXuLyHoSoID_DSFileDinhKem">
                <div class="box box-primary">
                    <div class="box-body">
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row panel" style="margin-bottom: 5px;">
        <div class="btn btn-default btn-block tab-doc" style="text-align:left;">
            <a style="color: blue;font-weight: bold;" data-toggle="collapse" data-parent="#divXuLyHoSoID" href="#collapse_XuLyHoSo_TTTraHoSo">
                Thông tin trả hồ sơ
            </a>
        </div>
        <div id="collapse_XuLyHoSo_TTTraHoSo" class="panel-collapse collapse">
            <div id="divXuLyHoSoID_TTTraHoSo">
                <div class="box box-primary">
                    <div class="box-body">
                        <div class="row">
                            <div class="col-xs-2">
                                @Html.LabelFor(model => model.TENNGUOINHANKETQUA, "Người nhận", new { @class = "pull-right control-label" })
                            </div>
                            <div class="col-xs-4">
                                @Html.TextBoxFor(model => model.TENNGUOINHANKETQUA, "", new { @class = "form-control input-sm" })
                            </div>
                            <div class="col-xs-2">
                                @*@Html.LabelFor(model => model.TUCACHNGUOINHANKETQUA, "Tư cách người nhận", new { @class = "pull-right control-label" })*@
                            </div>
                            <div class="col-xs-4">
                                @*@Html.TextBoxFor(model => model.TUCACHNGUOINHANKETQUA, "", new { @class = "form-control input-sm" })*@
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-2">
                                @Html.LabelFor(model => model.NGAYHENTRAHOSO, "Ngày hẹn trả", new { @class = "pull-right control-label" })
                            </div>
                            <div class="col-xs-4">
                                @Html.TextBoxFor(model => model.NGAYHENTRAHOSO, "{0:yyyy-MM-dd}", new { @type = "date", @class = "form-control input-sm" })
                            </div>
                            <div class="col-xs-2">
                                @Html.LabelFor(model => model.NGAYTRAKETQUA, "Ngày trả", new { @class = "pull-right control-label" })
                            </div>
                            <div class="col-xs-4">
                                @Html.TextBoxFor(model => model.NGAYTRAKETQUA, "{0:yyyy-MM-dd}", new { @type = "date", @class = "form-control input-sm" })
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-2">
                                @Html.LabelFor(model => model.DIACHINGUOINHANKETQUA, "Địa chỉ người nhận", new { @class = "pull-right control-label" })
                            </div>
                            <div class="col-xs-10">
                                @Html.TextAreaFor(model => model.DIACHINGUOINHANKETQUA, new { @class = "form-control", @rows = "3" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        //kích hoạt sự kiện trên các tab dọc
        trigger_tab_doc();
    })
</script>