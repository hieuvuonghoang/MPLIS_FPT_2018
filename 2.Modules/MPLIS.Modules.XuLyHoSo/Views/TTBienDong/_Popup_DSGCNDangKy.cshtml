@model MPLIS.Libraries.Data.XuLyHoSo.Models.DSGCNDonVM

<div class='modal fade' id='modalformDSGCNVAOID'>
    <div class='modal-dialog'>
        <div class='modal-content'>
            <div class='modal-header'>
                <button type='button' class='close' data-dismiss='modal'>&times;</button>
                <h4 class='modal-title'>Thêm giấy chứng nhận từ đăng ký</h4>
            </div>
            <div class='modal-body'>
                <table id="popuptable_DSGCNDangKyID" class="table table-bordered" data-toggle="table" style="margin-bottom:0px;">
                    <thead>
                        <tr>
                            <th>
                                <input type="checkbox" id="checkall_DSGCNDangKyID" />
                            </th>
                            <th>#</th>
                            <th>Số phát hành</th>
                            <th>Mã vạch</th>
                            <th>Trạng thái</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.DSDangKyGCN != null && Model.DSDangKyGCN.Count() > 0)
                        {
                            int rmoivao = 0;
                            foreach (var item in Model.DSDangKyGCN)
                            {
                                rmoivao = rmoivao + 1;
                                <tr data-id="@item.GIAYCHUNGNHANID">
                                    <td class="text-center">
                                        <input type="checkbox" name="DSGCNVAO_CHON" value="@item.GIAYCHUNGNHANID" />
                                    </td>
                                    <td class="text-right">
                                        @rmoivao.
                                    </td>
                                    <td>
                                        @item.SoPhatHanh
                                    </td>
                                    <td class="text-center">
                                        @item.MaVach
                                    </td>
                                    <td class="text-center">
                                        @item.TrangThai
                                    </td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
            <div class='modal-footer' style='text-align:center;'>
                <button type='button' class='t100 btn btn-primary btn-sm pull-right'>Lưu</button>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        tableDSGCNDangKy = InitDataTableXLHS($('#popuptable_DSGCNDangKyID'));
        ProcessCheckBox(tableDSGCNDangKy, $('#checkall_DSGCNDangKyID'));
    });
</script>
